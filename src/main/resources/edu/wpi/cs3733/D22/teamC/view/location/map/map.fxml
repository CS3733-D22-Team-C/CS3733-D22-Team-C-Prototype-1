<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<ScrollPane fx:id="scrollPane" prefViewportHeight="500.0" prefViewportWidth="500.0" style="-fx-background: #2e2e2e;" stylesheets="@../../../css/map.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <StackPane prefHeight="150.0" prefWidth="200.0">
         <children>
            <ImageView fx:id="mapImage" pickOnBounds="true" preserveRatio="true" />
            <AnchorPane fx:id="mapPane" prefHeight="200.0" prefWidth="200.0" />
         </children>
      </StackPane>
   </content>
</ScrollPane>
