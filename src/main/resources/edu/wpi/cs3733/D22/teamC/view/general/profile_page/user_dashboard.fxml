<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTreeTableView?>
<?import io.github.palexdev.materialfx.controls.MFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox id="page" maxHeight="1.7976931348623157E308" maxWidth="-Infinity" prefWidth="1100.0" stylesheets="@../../../css/profile.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.D22.teamC.controller.profile.UserDashboardController">
   <children>
      <HBox minHeight="-Infinity" minWidth="-Infinity" styleClass="card">
         <children>
            <VBox id="fields-container" alignment="CENTER">
               <children>
                  <ImageView fx:id="profileImage" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                  <MFXButton fx:id="addimg" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" onAction="#addImage" prefWidth="84.0" styleClass="vb_class" stylesheets="@../../../css/profile.css" text="Change Image" />
               </children>
            </VBox>
            <VBox id="text-box" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
               <children>
                  <Label id="greeting" fx:id="greetingLabel" alignment="TOP_CENTER" text="Hello, ________!" />
                  <HBox id="top-container" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                     <children>
                        <HBox id="fields-container" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                           <children>
                              <Label id="field-label" text="Contact:" HBox.hgrow="SOMETIMES" />
                              <Label id="field-label" fx:id="contact" text="Label" HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                        <HBox id="fields-container" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                           <children>
                              <Label id="field-label" text="Username:" />
                              <Label id="field-label" fx:id="username" text="Label" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
                           <children>
                              <MFXButton fx:id="back1" onAction="#backButton" styleClass="back_button" stylesheets="@../../../css/profile.css" text="Change Password" />
                           </children>
                        </HBox>
                     </children>
                  </HBox>
                  <HBox id="fields-container" alignment="BOTTOM_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                     <children>
                        <Label id="field-label" text="Address:" HBox.hgrow="SOMETIMES" />
                        <Label id="field-label" fx:id="address" text="Label" HBox.hgrow="ALWAYS" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
         <children>
            <TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@../../../css/default.css" tabClosingPolicy="UNAVAILABLE" HBox.hgrow="ALWAYS">
               <tabs>
                  <Tab closable="false" text="Assigned Service Requests">
                     <content>
                        <VBox fx:id="assignedTableBox">
                           <children>
                              <JFXTreeTableView fx:id="assignedTable" prefHeight="448.0" prefWidth="368.0" styleClass="table-view" />
                           </children>
                        </VBox>
                     </content>
                  </Tab>
                  <Tab closable="false" text="Created Service Requests">
                     <content>
                        <VBox fx:id="createdTableBox">
                           <children>
                              <JFXTreeTableView fx:id="createdTable" styleClass="table-view" />
                           </children>
                        </VBox>
                     </content>
                  </Tab>
               </tabs>
               <effect>
                  <DropShadow />
               </effect>
            </TabPane>
         </children>
      </HBox>
      <MFXButton fx:id="back" onAction="#backButton" styleClass="back_button" stylesheets="@../../../css/profile.css" text="Back" />
   </children>
</VBox>
